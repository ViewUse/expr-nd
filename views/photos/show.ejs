<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf8">
		<title><%= title %></title>
		<link rel="stylesheet" href="stylesheets/pic-list.css" />
	</head>
	<body>
		<div id="photos">
			<% photos.forEach(function(photo) { %>
					<div class="photo">
						<h2><%= photo.name %></h2>
						<img src='/photos/<%=photo.name%>.png' />
					</div>
			<% }) %>
		</div>
	</body>
</html>